{
  "short_name": "Aria2 Integration Extension",
  "name": "Aria2 Integration Extension",
  "icons": {
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "commands": {
    "open_detail": {
      "description": "Open AriaNg"
    }
  },
  "background": {
    "scripts": ["target/background/index.js"]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "contextMenus",
    "notifications",
    "storage",
    "downloads",
    "downloads.open",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "homepage_url": "https://addons.mozilla.org/en-US/firefox/addon/aria2-integration-extension/",
  "manifest_version": 2,
  "version": "0.2.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Aria2Ex"
  }
}
